<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="assets_backend" name="point_of_sale assets" inherit_id="point_of_sale.assets_backend">
            <xpath expr="//script[contains(@src,'models.js')]" position="replace">
                   <script type="text/javascript" src="/granpat_conf/static/src/js/models.js"></script>
            </xpath>
        </template>

        <template id="index_ga" name="POS Index GA" inherit_id="point_of_sale.index">

            <xpath expr="//link[contains(@href,'pos.css')]" position="replace">
                <link rel="stylesheet" href="/granpat_conf/static/src/css/pos.css" id="pos-stylesheet"/>
            </xpath>
        </template>


    </data>
</openerp>